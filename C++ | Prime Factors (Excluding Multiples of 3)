#include <iostream>
using namespace std;

int main() {
    int number;
    cout << "Enter a number: ";
    cin >> number;

    cout << "Prime factors of " << number << " (excluding multiples of 3): ";

    for (int counter = 2; counter <= number; counter++) {
        if (counter % 3 == 0)
        {
            continue;
        }
        else
        {
            while (number % counter == 0) 
            {
                cout << counter << " ";
                number /= counter;
            }
        }
    }
}
